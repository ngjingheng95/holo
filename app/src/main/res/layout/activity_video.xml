<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChromaKeyVideoActivity">

  <fragment
      android:id="@+id/ux_fragment"
      class="com.google.ar.sceneform.samples.chromakeyvideo.WritingArFragment"
      android:layout_width="match_parent"
      android:layout_height="match_parent" />
  <RelativeLayout
      android:id="@+id/playback_group"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      >
    <android.support.design.widget.FloatingActionButton
        android:id="@+id/create_holo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="center"
        android:layout_alignBottom="@+id/playback_play_pause"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        android:layout_toLeftOf="@+id/playback_play_pause"
        android:clickable="true"
        android:src="@drawable/create_ar"
        app:backgroundTint="@color/ultraLightGrey"
        app:fabSize="normal"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/playback_play_pause"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/capture_video"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        android:layout_toLeftOf="@+id/capture_video"
        android:clickable="true"
        android:src="@drawable/ic_pause_white"
        android:tint="@color/darkGrey"
        app:backgroundTint="@color/ultraLightGrey"
        android:visibility="invisible"
        app:fabSize="mini"/>


    <android.support.design.widget.FloatingActionButton
        android:id="@+id/capture_photo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/capture_video"
        android:layout_marginBottom="8dp"
        android:layout_marginLeft="8dp"
        android:layout_toRightOf="@+id/capture_video"
        android:clickable="true"
        android:src="@drawable/ic_photo_camera_24dp"
        android:tint="@color/darkGrey"
        app:backgroundTint="@color/ultraLightGrey"
        app:fabSize="mini"
        android:visibility="invisible"/>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/capture_video"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="25dp"
        app:elevation="1dp"
        android:clickable="true"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintHorizontal_chainStyle="spread"
        app:srcCompat="@drawable/round_videocam"
        android:tint="@color/darkGrey"
        app:backgroundTint="@color/ultraLightGrey"
        app:fabSize="normal"/>

  </RelativeLayout>
  <RelativeLayout
      android:id="@+id/placement_group"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:clipChildren="false"
      android:clipToPadding="false"
      android:elevation="12dp">

        <View
            android:id="@+id/placement_menu_background"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:alpha=".35"
            android:background="@android:color/black"/>

      <android.support.v7.widget.CardView
          android:id="@+id/placement_menu"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentEnd="false"
          android:layout_marginTop="500dp"
          android:paddingTop="16dp"
          android:paddingBottom="16dp"
          android:translationX="16dp"
          app:cardCornerRadius="8dp">

          <android.support.design.widget.TabLayout
              android:id="@+id/tabs"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              app:tabGravity="fill"
              app:tabMode="fixed">

              <android.support.design.widget.TabItem
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="My Holos" />

              <android.support.design.widget.TabItem
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="Featured" />

              <android.support.design.widget.TabItem
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="Characters" />

              <android.support.design.widget.TabItem
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="Animals" />

              <android.support.design.widget.TabItem
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="Pixabay" />

          </android.support.design.widget.TabLayout>

          <ScrollView
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:layout_below="@id/tabs"
              android:layout_marginTop="100dp">

              <RelativeLayout
                  android:layout_width="match_parent"
                  android:layout_height="match_parent"
                  android:layout_below="@id/tabs"
                  android:orientation="vertical">

                  <com.google.ar.sceneform.samples.chromakeyvideo.DynamicGridView
                      android:id="@+id/gallery"
                      android:layout_width="match_parent"
                      android:layout_height="700dp"
                      android:layout_below="@+id/tabs"
                      android:gravity="center"
                      android:horizontalSpacing="1dp"
                      android:numColumns="4"
                      android:stretchMode="columnWidth"
                      android:verticalSpacing="1dp"
                      tools:ignore="NotSibling" />

                  <com.google.ar.sceneform.samples.chromakeyvideo.DynamicGridView
                      android:id="@+id/gallery2"
                      android:layout_width="match_parent"
                      android:layout_height="700dp"
                      android:layout_below="@id/tabs"
                      android:gravity="center"
                      android:horizontalSpacing="1dp"
                      android:numColumns="4"
                      android:stretchMode="columnWidth"
                      android:verticalSpacing="1dp"
                      android:visibility="gone"
                      tools:ignore="NotSibling" />

                  <com.google.ar.sceneform.samples.chromakeyvideo.DynamicGridView
                      android:id="@+id/gallery3"
                      android:layout_width="match_parent"
                      android:layout_height="700dp"
                      android:layout_below="@id/tabs"
                      android:gravity="center"
                      android:horizontalSpacing="1dp"
                      android:numColumns="4"
                      android:stretchMode="columnWidth"
                      android:verticalSpacing="1dp"
                      android:visibility="gone"
                      tools:ignore="NotSibling" />

                  <com.google.ar.sceneform.samples.chromakeyvideo.DynamicGridView
                      android:id="@+id/gallery4"
                      android:layout_width="match_parent"
                      android:layout_height="700dp"
                      android:layout_below="@id/tabs"
                      android:gravity="center"
                      android:horizontalSpacing="1dp"
                      android:numColumns="4"
                      android:stretchMode="columnWidth"
                      android:verticalSpacing="1dp"
                      android:visibility="gone"
                      tools:ignore="NotSibling" />


                  <EditText
                      android:layout_width="match_parent"
                      android:layout_height="match_parent"
                      android:visibility="gone"
                      android:layout_toLeftOf="@+id/pixabaySearchButton"
                      android:layout_toRightOf="@id/pixabaySearchBackButton"
                      android:id="@+id/pixabaySearchQuery"
                      android:hint="Search Pixabay"
                      />

                  <Button
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:visibility="gone"
                      android:id="@+id/pixabaySearchButton"
                      android:layout_alignParentRight="true"
                      android:text="Search"
                      android:layout_alignBottom="@+id/pixabaySearchQuery"
                      android:layout_alignTop="@id/pixabaySearchQuery"
                      />

                  <Button
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:visibility="gone"
                      android:id="@+id/pixabaySearchBackButton"
                      android:onClick="onClickPixabayBackButton"
                      android:layout_alignParentLeft="true"
                      android:text="Back"
                      android:layout_alignBottom="@+id/pixabaySearchQuery"
                      android:layout_alignTop="@id/pixabaySearchQuery"
                      />

                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:id="@+id/pixabaySearchLoadingText"
                      android:visibility="gone"
                      android:text="Loading..."
                      android:textStyle="bold"
                      android:layout_alignBottom="@+id/pixabaySearchBackButton"
                      android:layout_alignTop="@+id/pixabaySearchBackButton"
                      android:layout_centerHorizontal="true"
                      />


                  <com.google.ar.sceneform.samples.chromakeyvideo.DynamicGridView
                      android:id="@+id/gallery5"
                      android:layout_width="match_parent"
                      android:layout_height="700dp"
                      android:layout_below="@id/pixabaySearchBackButton"
                      android:gravity="center"
                      android:horizontalSpacing="1dp"
                      android:numColumns="4"
                      android:stretchMode="columnWidth"
                      android:verticalSpacing="1dp"
                      android:visibility="gone" />

                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:layout_below="@+id/pixabaySearchQuery"
                      android:layout_marginTop="20dp"
                      android:layout_marginBottom="20dp"
                      android:layout_marginLeft="20dp"
                      android:layout_alignParentLeft="true"
                      android:text="Or Search by Categories..."
                      android:visibility="gone"
                      android:id="@+id/infoText"/>

                  <com.google.ar.sceneform.samples.chromakeyvideo.DynamicGridView
                      android:id="@+id/pixabayCategories"
                      android:layout_width="match_parent"
                      android:layout_height="700dp"
                      android:layout_below="@id/infoText"
                      android:gravity="center"
                      android:horizontalSpacing="1dp"
                      android:numColumns="4"
                      android:stretchMode="columnWidth"
                      android:verticalSpacing="1dp"
                      android:visibility="gone"
                      />

              </RelativeLayout>

          </ScrollView>
      </android.support.v7.widget.CardView>

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/placement_fab"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="25dp"
        android:layout_marginEnd="23dp"
        app:backgroundTint="@color/colorPrimary"
        android:clickable="true"
        android:src="@drawable/ic_add_white"
        app:elevation="16dp"
        app:fabSize="normal"/>


      <android.support.design.widget.FloatingActionButton
          android:id="@+id/test_fab"
          android:layout_width="56dp"
          android:layout_height="56dp"
          android:layout_alignParentTop="true"
          android:layout_alignParentEnd="true"
          android:layout_marginBottom="25dp"
          android:layout_marginEnd="23dp"
          app:backgroundTint="@color/recordRed"
          android:clickable="true"
          android:onClick="onClickMainTabView"
          app:elevation="16dp"
          app:fabSize="mini"/>


      <RelativeLayout
          android:layout_height="match_parent"
          android:layout_width="match_parent"
          android:visibility="gone"
          android:animateLayoutChanges="true"
          android:id="@+id/main_tab_group">
          <RelativeLayout
              android:id="@+id/main_tab_fade"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:alpha=".35"
              android:background="@android:color/black">
<!--              <ImageView-->
<!--                  android:layout_width="wrap_content"-->
<!--                  android:layout_height="wrap_content"-->
<!--                  android:layout_centerInParent="true"-->
<!--                  android:src="@drawable/move_phone_icon"-->
<!--                  android:id="@+id/move_phone_icon"/>-->
          </RelativeLayout>

          <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_above="@id/main_tab"
              android:background="@color/ultraLightGrey"
              android:id="@+id/main_tab_header_1"
              >

              <RelativeLayout
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:clickable="true"
                  android:onClick="onClickBackView"
                  android:layout_alignParentLeft="true"
                  android:layout_marginVertical="10dp"
                  android:layout_marginHorizontal="15dp"
                  android:layout_centerVertical="true"
                  android:id="@+id/back_button"
                  android:visibility="gone">

                  <ImageView
                      android:layout_width="20dp"
                      android:layout_height="20dp"
                      android:id="@+id/back_button_icon"
                      android:src="@drawable/back_icon"
                      android:layout_centerVertical="true"/>
                  <TextView
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:text="Back"
                      android:layout_centerVertical="true"
                      android:textColor="#000"
                      android:layout_toRightOf="@+id/back_button_icon"
                      />

              </RelativeLayout>

              <ImageView
                  android:layout_width="20dp"
                  android:layout_height="20dp"
                  android:src="@drawable/close_icon"
                  android:layout_alignParentRight="true"
                  android:layout_marginVertical="10dp"
                  android:layout_marginHorizontal="15dp"
                  android:layout_centerVertical="true"
                  android:id="@+id/close_button"
                  android:clickable="true"
                  android:onClick="onClickCloseView"/>

              <RelativeLayout
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:id="@+id/pixabay_search_bar"
                  android:visibility="gone"
                  android:layout_marginHorizontal="50dp"
                  android:layout_centerVertical="true"
                  android:layout_toLeftOf="@id/close_button">



                  <android.support.v7.widget.AppCompatEditText
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:id="@+id/pixabay_search_text"
                      android:hint="Search Holos on Pixabay   "
                      android:backgroundTint="@color/gray_bg"
                      android:textColorHint="@color/gray_bg"
                      android:textColor="@color/gray_overlay"
                      />

                  <RelativeLayout
                      android:layout_width="wrap_content"
                      android:layout_height="wrap_content"
                      android:id="@+id/pixabay_search_btn"
                      android:layout_toRightOf="@id/pixabay_search_text"
                      android:layout_centerVertical="true"
                      android:layout_marginHorizontal="10dp">

                      <ImageView
                          android:layout_width="25dp"
                          android:layout_height="25dp"
                          android:src="@drawable/search_icon"
                          android:layout_centerInParent="true"/>

                  </RelativeLayout>





              </RelativeLayout>



          </RelativeLayout>




          <RelativeLayout
              android:layout_width="match_parent"
              android:layout_height="177dp"
              android:layout_alignParentBottom="true"
              android:background="#FFF"
              android:id="@+id/main_tab">

              <!--       New Tab       -->
              <android.support.v7.widget.RecyclerView
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:id="@+id/new_tab"
                  android:visibility="gone"
                  android:scrollbars="horizontal"
                  android:scrollbarSize="5dp"/>

              <!--       Animals Tab       -->
              <android.support.v7.widget.RecyclerView
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:id="@+id/animals_tab"
                  android:visibility="gone"
                  android:scrollbars="horizontal"
                  android:scrollbarSize="5dp"/>

              <!--       Characters Tab       -->
              <android.support.v7.widget.RecyclerView
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:id="@+id/characters_tab"
                  android:visibility="gone"
                  android:scrollbars="horizontal"
                  android:scrollbarSize="5dp"/>

              <!--       My Holos Tab       -->
              <android.support.v7.widget.RecyclerView
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:id="@+id/my_holos_tab"
                  android:visibility="gone"
                  android:scrollbars="horizontal"
                  android:scrollbarSize="5dp"/>

              <!--       Pixabay Tab       -->
              <android.support.v7.widget.RecyclerView
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:scrollbars="horizontal"
                  android:id="@+id/pixabay_tab"
                  android:visibility="gone"
                  android:scrollbarSize="5dp"/>


              <!--      Buttons Layout        -->
              <LinearLayout
                  android:layout_width="match_parent"
                  android:layout_height="wrap_content"
                  android:orientation="vertical"
                  android:padding="10dp"
                  android:visibility="visible"
                  android:id="@+id/buttons_tab"
                  >

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="0dp"
                      android:layout_weight="1"
                      android:orientation="horizontal"
                      android:id="@+id/buttons_row_1">

                      <LinearLayout
                          android:layout_width="0dp"
                          android:layout_weight="1"
                          android:layout_height="wrap_content"
                          android:orientation="vertical"
                          android:background="@drawable/bg_browse_holos"
                          android:padding="10dp"
                          android:layout_margin="3dp"
                          android:clickable="true"
                          android:onClick="onClickNewView"
                          android:elevation="2dp"
                          >

                          <ImageView
                              android:src="@drawable/new_icon"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_gravity="center_horizontal"/>

                          <TextView
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="New!"
                              android:textColor="#000"
                              android:layout_gravity="center_horizontal"
                              android:layout_marginTop="5dp"/>


                      </LinearLayout>

                      <LinearLayout
                          android:layout_width="0dp"
                          android:layout_weight="1"
                          android:layout_height="wrap_content"
                          android:orientation="vertical"
                          android:background="@drawable/bg_browse_holos"
                          android:padding="10dp"
                          android:layout_margin="3dp"
                          android:clickable="true"
                          android:onClick="onClickAnimalsView"
                          android:elevation="2dp"
                          >

                          <ImageView
                              android:src="@drawable/tiger_icon"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_gravity="center_horizontal"/>

                          <TextView
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="Animals"
                              android:textColor="#000"
                              android:layout_gravity="center_horizontal"
                              android:layout_marginTop="5dp"/>


                      </LinearLayout>

                      <LinearLayout
                          android:layout_width="0dp"
                          android:layout_weight="1"
                          android:layout_height="wrap_content"
                          android:orientation="vertical"
                          android:background="@drawable/bg_browse_holos"
                          android:padding="10dp"
                          android:layout_margin="3dp"
                          android:clickable="true"
                          android:onClick="onClickCharactersView"
                          android:elevation="2dp"
                          >

                          <ImageView
                              android:src="@drawable/superhero_icon"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_gravity="center_horizontal"/>

                          <TextView
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="Characters"
                              android:textColor="#000"
                              android:layout_gravity="center_horizontal"
                              android:layout_marginTop="5dp"/>


                      </LinearLayout>


                  </LinearLayout>

                  <LinearLayout
                      android:layout_width="match_parent"
                      android:layout_height="0dp"
                      android:layout_weight="1"
                      android:orientation="horizontal"
                      android:id="@+id/buttons_row_2">

                      <LinearLayout
                          android:layout_width="0dp"
                          android:layout_weight="1"
                          android:layout_height="wrap_content"
                          android:orientation="vertical"
                          android:background="@drawable/bg_browse_holos"
                          android:padding="10dp"
                          android:layout_margin="3dp"
                          android:clickable="true"
                          android:onClick="onClickMyHoloView"
                          android:elevation="2dp"
                          >

                          <ImageView
                              android:src="@drawable/idea_icon"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_gravity="center_horizontal"/>

                          <TextView
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="My Holos"
                              android:textColor="#000"
                              android:layout_gravity="center_horizontal"
                              android:layout_marginTop="5dp"/>


                      </LinearLayout>

                      <LinearLayout
                          android:layout_width="0dp"
                          android:layout_weight="1"
                          android:layout_height="wrap_content"
                          android:orientation="vertical"
                          android:background="@drawable/bg_browse_holos"
                          android:padding="10dp"
                          android:layout_margin="3dp"
                          android:clickable="true"
                          android:onClick="onClickPixabayView"
                          android:elevation="2dp"
                          >

                          <ImageView
                              android:src="@drawable/pixabay_logo"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_gravity="center_horizontal"/>

                          <TextView
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="Search Pixabay"
                              android:textColor="#000"
                              android:layout_gravity="center_horizontal"
                              android:layout_marginTop="5dp"/>


                      </LinearLayout>

                      <LinearLayout
                          android:layout_width="0dp"
                          android:layout_weight="1"
                          android:layout_height="wrap_content"
                          android:orientation="vertical"
                          android:background="@drawable/bg_browse_holos"
                          android:padding="10dp"
                          android:layout_margin="3dp"
                          android:clickable="true"
                          android:onClick="onClickTest"
                          android:elevation="2dp"
                          >

                          <ImageView
                              android:src="@drawable/yt_logo_rgb_light2"
                              android:layout_width="30dp"
                              android:layout_height="30dp"
                              android:layout_gravity="center_horizontal"/>

                          <TextView
                              android:layout_width="wrap_content"
                              android:layout_height="wrap_content"
                              android:text="Search Youtube"
                              android:textColor="#000"
                              android:layout_gravity="center_horizontal"
                              android:layout_marginTop="5dp"/>


                      </LinearLayout>


                  </LinearLayout>

              </LinearLayout>
          </RelativeLayout>

      </RelativeLayout>



